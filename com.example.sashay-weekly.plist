<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.example.sashay-weekly</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/sashay.py</string>
		<string>-s</string>
		<string>'Weekly'</string>
        <string>-f</string>
        <string>7</string>
	</array>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Weekday</key>
		<integer>1</integer>
		<key>Hour</key>
		<integer>8</integer>
		<key>Minute</key>
		<integer>15</integer>
	</dict>
</dict>
</plist>